{
  "name": "git@gitlab.com:mianmohammad/game",
  "description": "Game Project CLI",
  "license": "proprietary",
  "type": "project",
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*",
    "symfony/console": "~2.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "app/Core/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true
  }
}
